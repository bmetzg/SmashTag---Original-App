# SmashTag

use_frameworks!
platform :ios, '8.0'

pod 'Firebase/Storage'
pod 'Firebase/Core'
pod 'Firebase/Database'
pod 'Firebase/RemoteConfig'
pod 'FirebaseUI'

source 'https://github.com/CocoaPods/Specs.git'
target 'SmashTag' do
    pod 'GooglePlaces'
    pod 'GooglePlacePicker'
    pod 'GoogleMaps'
end
